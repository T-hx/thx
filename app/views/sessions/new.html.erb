<% provide(:title, 'ログイン') %>
<h1>ログイン</h1>

<dev>
  <div>
    <%= form_for(:session, url: login_path) do |f| %>
      <%= f.label :email %>
      <%= f.email_field :email %>

      <%= f.label :password %>
      <%= f.password_field :password %>

      <%= f.submit 'ログイン' %>
    <% end %>

    <p><%= link_to '新規登録', signup_path %></p>
  </div>
</dev>